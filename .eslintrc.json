{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "plugin:prettier/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "brace-style": "error",
        "block-scoped-var": "off",
        "default-case": "off",
        "no-div-regex": "off",
        "no-var": "error",
        "vars-on-top": "off"
    }
}
